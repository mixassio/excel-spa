{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"jest/globals": true
	},
	"rules": {
		"semi": "off",
		"require-jsdoc": "off",
    "object-curly-spacing": "off",
		"arrow-parens": "off",
		"operator-linebreak": "off",
		"max-len": "off",
		"no-unused-vars": "off",
		"no-case-declarations": "off"
	},
	"plugins": [
		"jest"
	],
	"extends": ["eslint:recommended", "google"]
}